  "license": "ISC",
  "dependencies": {
    "bignumber.js": "^4.1.0",
    "tslint": "^5.8.0",
    "typescript": "^2.6.1",
    "web3": "0.19.0"
  },
  "devDependencies": {
    "@types/bignumber.js": "^4.0.3",
    "@types/chai": "^4.0.5",
    "chai": "^4.1.2",
    "chai-bignumber": "^2.0.2",
    "copyfiles": "^1.2.0",
    "web3-typescript-typings": "^0.7.2"
  }
}
